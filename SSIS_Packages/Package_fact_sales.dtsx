                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH LastHistoryCTE AS (
    SELECT 
        history_id,
        order_id,
        status_id AS last_status,
        status_date,
        ROW_NUMBER() OVER (PARTITION BY order_id ORDER BY status_date DESC) AS rn
    FROM dbo.order_history
)
SELECT
    co.order_id,
    co.customer_id AS customer_fk,
    co.shipping_method_id AS shipping_method_fk,
    ol.book_id AS book_fk,
CAST(co.order_date AS DATE) AS date,
CAST(co.order_date AS TIME(0)) AS time,

 
    isnull(lh.history_id,0) AS history_fk,
    isnull(lh.last_status,5) as last_status, 
    ol.price,
    sm.cost AS shipping_cost
FROM dbo.cust_order co
LEFT JOIN dbo.order_line ol ON co.order_id = ol.order_id
LEFT JOIN LastHistoryCTE lh ON co.order_id = lh.order_id AND lh.rn = 1
LEFT JOIN dbo.shipping_method sm ON co.shipping_method_id = sm.method_id</property>
